<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" targetNamespace="http://skat.dk/begrebsmodel/2009/01/15/" elementFormDefault="qualified" version="1.0" xmlns:fase2.1="http://skat.dk/begrebsmodel/2009/01/15/">
      
   
   
   <xs:simpleType name="KundeNummerType">
         
      
      
      <xs:annotation>
            
         
         
         <xs:documentation>Et nummer, som for SKAT identificerer en person, virksomhed eller udenlandsk samarbejdspart. En virksomhed er identificeret ved et CVR-nr. eller SE-nr. (eller begge), mens en person er identificeret af CPR-nr.</xs:documentation>
         
      
      
      </xs:annotation>
         
      
      
      <xs:restriction base="xs:string">
            
         
         
         <xs:maxLength value="11"/>
            
         
         
         <xs:pattern value="[0-9]{8,11}"/>
         
      
      
      </xs:restriction>
      
   
   
   </xs:simpleType>
   


</xs:schema>
