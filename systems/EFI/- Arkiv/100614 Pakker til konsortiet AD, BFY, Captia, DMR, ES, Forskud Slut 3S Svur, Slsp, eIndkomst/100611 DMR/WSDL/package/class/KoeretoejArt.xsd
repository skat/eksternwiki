   </xs:element>
      
   
   
   <xs:element name="KøretøjArtGyldigTil" type="fase2.1:DatoType">
      
   
   
   </xs:element>
      
   
   
   <xs:element name="KøretøjArtStatus" type="fase2.1:ForretningParameterVærdiStatusType">
      
   
   
   </xs:element>



</xs:schema>
