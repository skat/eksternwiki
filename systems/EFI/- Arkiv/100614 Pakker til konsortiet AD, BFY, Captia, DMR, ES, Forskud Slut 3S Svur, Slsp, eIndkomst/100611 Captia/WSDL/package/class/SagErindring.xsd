   </xs:annotation>
      
   
   
   <xs:include schemaLocation="../types/DatoType.xsd"/>
      
   
   
   <xs:include schemaLocation="../types/Tekst255Type.xsd"/>
      
   
   
   <xs:element name="SagErindringDato" type="fase2.1:DatoType">
         
      
      
      <xs:annotation>
            
         
         
         <xs:documentation>Erindrer sagen i Captia og Outlook Today</xs:documentation>
         
      
      
      </xs:annotation>
      
   
   
   </xs:element>
      
   
   
   <xs:element name="SagErindringBeskrivelse" type="fase2.1:Tekst255Type">
         
      
      
      <xs:annotation>
            
         
         
         <xs:documentation>Beskrivelse af en erindring på en sag.</xs:documentation>
         
      
      
      </xs:annotation>
      
   
   
   </xs:element>



</xs:schema>
