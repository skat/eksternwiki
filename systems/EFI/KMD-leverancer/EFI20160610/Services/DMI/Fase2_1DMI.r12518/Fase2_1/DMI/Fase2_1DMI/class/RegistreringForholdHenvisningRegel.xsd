<?xml version="1.0" encoding="UTF-8"?>
      <xs:schema xmlns:fase2.1="http://skat.dk/begrebsmodel/2009/01/15/"
                 xmlns:xs="http://www.w3.org/2001/XMLSchema"
                 targetNamespace="http://skat.dk/begrebsmodel/2009/01/15/"
                 elementFormDefault="qualified"
                 attributeFormDefault="unqualified"
                 version="1.0">
         <xs:annotation>
               <xs:documentation>Angiver lovlige kombinationer mellem henvisningstype og pligt. Henvisninger af typen fællesafregning kan eksempelvis kun registreres for pligterne moms, lønsum, A-skat og enkelte punktafgifter. Sambeskatning kan kun registreres for pligterne Selskabs- og Fondsskat. </xs:documentation>
            </xs:annotation>
         <xs:include schemaLocation="../types/DatoType.xsd"/>
         <xs:element name="RegistreringForholdHenvisningRegelGyldigFra" type="fase2.1:DatoType">
               
               <xs:annotation>
                  <xs:documentation>Angiver første dag en given regel gælder </xs:documentation>
               </xs:annotation>
            </xs:element>
         <xs:element name="RegistreringForholdHenvisningRegelGyldigTil" type="fase2.1:DatoType">
               
               <xs:annotation>
                  <xs:documentation>Angiver sidste dag en given regel gælder </xs:documentation>
               </xs:annotation>
            </xs:element>
      </xs:schema>
