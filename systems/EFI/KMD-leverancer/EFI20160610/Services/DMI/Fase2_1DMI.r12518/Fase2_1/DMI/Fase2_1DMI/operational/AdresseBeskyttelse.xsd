<?xml version="1.0" encoding="UTF-8"?>
      <xs:schema xmlns:fase2.1="http://skat.dk/begrebsmodel/2009/01/15/"
                 xmlns:xs="http://www.w3.org/2001/XMLSchema"
                 targetNamespace="http://skat.dk/begrebsmodel/2009/01/15/"
                 elementFormDefault="qualified"
                 version="1.0">
         <xs:include schemaLocation="../types/MarkeringType.xsd"/>
         
         <xs:element name="AdresseBeskyttelse" type="fase2.1:MarkeringType">
            <xs:annotation>
               <xs:documentation>Angiver om en persons eller virksomheds adresse er beskyttet for offentligheden. (Markeringen er sat i hhv. CPR- og CVR-registeret.) Det er således kun myndigheder med lovmæssigt grundlag, som har adgang til adressen (fx i forbindelse med sagsbehandling). (true = beskyttet, false = ikke beskyttet)</xs:documentation>
            </xs:annotation>
         </xs:element>
      </xs:schema>
