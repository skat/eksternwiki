<?xml version="1.0" encoding="UTF-8"?>
      
      <xs:schema xmlns:fase2.1="http://skat.dk/begrebsmodel/2009/01/15/"
                 xmlns:xs="http://www.w3.org/2001/XMLSchema"
                 targetNamespace="http://skat.dk/begrebsmodel/2009/01/15/"
                 elementFormDefault="qualified"
                 version="1.0">
            
            <xs:simpleType name="AkselTypeType">
               <xs:annotation>
                  <xs:documentation>Akseltype i form af hjulmontering for akslen med størst akseltryk.</xs:documentation>
               </xs:annotation>
               <xs:restriction base="xs:string">
                  <xs:maxLength value="40"/>
                  <xs:enumeration value="Enkeltmontering"/>
                  <xs:enumeration value="Storvolumendæk"/>
                  <xs:enumeration value="Dobbeltmontering"/>
                  <xs:enumeration value="Pendelaksel"/>
               </xs:restriction>
            </xs:simpleType>
         </xs:schema>
