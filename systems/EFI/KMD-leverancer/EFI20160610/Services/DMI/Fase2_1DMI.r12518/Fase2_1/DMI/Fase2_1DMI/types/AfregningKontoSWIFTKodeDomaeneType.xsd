<?xml version="1.0" encoding="UTF-8"?>
      
      <xs:schema xmlns:fase2.1="http://skat.dk/begrebsmodel/2009/01/15/"
                 xmlns:xs="http://www.w3.org/2001/XMLSchema"
                 targetNamespace="http://skat.dk/begrebsmodel/2009/01/15/"
                 elementFormDefault="qualified"
                 version="1.0">
            
            <xs:simpleType name="AfregningKontoSWIFTKodeDomæneType">
               <xs:annotation>
                  <xs:documentation>En SWIFT kode der maksimalt kan tage 11 karakterer jf. SWIFT standarden (ISO 9362). Kaldes også Bank Identifier Code (BIC)</xs:documentation>
               </xs:annotation>
               <xs:restriction base="xs:string">
                  <xs:maxLength value="11"/>
               </xs:restriction>
            </xs:simpleType>
         </xs:schema>
