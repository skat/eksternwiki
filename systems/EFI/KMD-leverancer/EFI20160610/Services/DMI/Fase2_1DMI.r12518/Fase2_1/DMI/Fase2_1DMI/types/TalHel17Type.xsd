<?xml version="1.0" encoding="UTF-8"?>
      
      <xs:schema xmlns:fase2.1="http://skat.dk/begrebsmodel/2009/01/15/"
                 xmlns:xs="http://www.w3.org/2001/XMLSchema"
                 targetNamespace="http://skat.dk/begrebsmodel/2009/01/15/"
                 elementFormDefault="qualified"
                 version="1.0">
            
            <xs:simpleType name="TalHel17Type">
               <xs:annotation>
                  <xs:documentation>Et positivt heltal med 1-17 cifre (eller 0). 0 - 99.999.999.999.999.999</xs:documentation>
               </xs:annotation>
               <xs:restriction base="xs:integer">
                  <xs:minInclusive value="0"/>
                  <xs:totalDigits value="17"/>
               </xs:restriction>
            </xs:simpleType>
         </xs:schema>
