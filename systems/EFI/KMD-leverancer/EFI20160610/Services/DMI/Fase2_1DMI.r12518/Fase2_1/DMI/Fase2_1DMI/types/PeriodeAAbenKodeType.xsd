<?xml version="1.0" encoding="UTF-8"?>
      
      <xs:schema xmlns:fase2.1="http://skat.dk/begrebsmodel/2009/01/15/"
                 xmlns:xs="http://www.w3.org/2001/XMLSchema"
                 targetNamespace="http://skat.dk/begrebsmodel/2009/01/15/"
                 elementFormDefault="qualified"
                 version="1.0">
            
            <xs:simpleType name="PeriodeÅbenKodeType">
               <xs:annotation>
                  <xs:documentation>Angiver om en periode er  - åben for angivelse og betaling, eller  - åben for betaling (angivelse er tidligere modtaget).  Anvendes i forbindelse med punktafgifter vedrørende cigarer og chokolade, hvor der kan betales på et senere tidspunkt end der angives.</xs:documentation>
               </xs:annotation>
               <xs:restriction base="xs:string">
                  <xs:maxLength value="40"/>
                  <xs:enumeration value="0_PeriodenErÅbenForAngivelseOgBetaling"/>
                  <xs:enumeration value="1_PeriodenErÅbenForBetaling"/>
               </xs:restriction>
            </xs:simpleType>
         </xs:schema>
