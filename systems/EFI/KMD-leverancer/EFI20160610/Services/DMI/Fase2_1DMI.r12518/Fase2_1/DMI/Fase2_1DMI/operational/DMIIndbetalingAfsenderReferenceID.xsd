<?xml version="1.0" encoding="UTF-8"?>
      <xs:schema xmlns:fase2.1="http://skat.dk/begrebsmodel/2009/01/15/"
                 xmlns:xs="http://www.w3.org/2001/XMLSchema"
                 targetNamespace="http://skat.dk/begrebsmodel/2009/01/15/"
                 elementFormDefault="qualified"
                 version="1.0">
         <xs:include schemaLocation="../types/Tekst18Type.xsd"/>
         
         <xs:element name="DMIIndbetalingAfsenderReferenceID" type="fase2.1:Tekst18Type">
            <xs:annotation>
               <xs:documentation>Feltet benyttes af det afsendende system til at identificere den pågældende indbetaling.  Feltet skal være unikt indenfor et servicekald, men det afsendende system kan i øvrigt frit bestemme indholdet. Feltet benyttes til at svare om der er felt i den enkelte forsendelse.</xs:documentation>
            </xs:annotation>
         </xs:element>
      </xs:schema>
