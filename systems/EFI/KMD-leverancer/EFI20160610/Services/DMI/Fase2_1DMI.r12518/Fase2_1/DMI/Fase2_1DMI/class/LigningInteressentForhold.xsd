<?xml version="1.0" encoding="UTF-8"?>
      <xs:schema xmlns:fase2.1="http://skat.dk/begrebsmodel/2009/01/15/"
                 xmlns:xs="http://www.w3.org/2001/XMLSchema"
                 targetNamespace="http://skat.dk/begrebsmodel/2009/01/15/"
                 elementFormDefault="qualified"
                 attributeFormDefault="unqualified"
                 version="1.0">
         <xs:annotation>
               <xs:documentation>Angiver hvilken ejer, som i en given periode har været ligningsinteressent for en virksomhed. En ligningsinteressent er den ejer, som SKAT blandt flere retter henvendelse til om ligningsmæssige forhold.</xs:documentation>
            </xs:annotation>
         <xs:include schemaLocation="../types/DatoType.xsd"/>
         <xs:element name="LigningInteressentForholdGyldigFra" type="fase2.1:DatoType">
               
               <xs:annotation>
                  <xs:documentation>Angiver første dag et forhold gælder.</xs:documentation>
               </xs:annotation>
            </xs:element>
         <xs:element name="LigningInteressentForholdGyldigTil" type="fase2.1:DatoType">
               
               <xs:annotation>
                  <xs:documentation>Angiver sidste dag et forhold gælder.</xs:documentation>
               </xs:annotation>
            </xs:element>
      </xs:schema>
