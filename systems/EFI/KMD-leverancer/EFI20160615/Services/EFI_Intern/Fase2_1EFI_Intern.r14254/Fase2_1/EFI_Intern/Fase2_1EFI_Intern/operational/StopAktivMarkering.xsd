<?xml version="1.0" encoding="UTF-8"?>
      <xs:schema xmlns:fase2.1="http://skat.dk/begrebsmodel/2009/01/15/"
                 xmlns:xs="http://www.w3.org/2001/XMLSchema"
                 targetNamespace="http://skat.dk/begrebsmodel/2009/01/15/"
                 elementFormDefault="qualified"
                 version="1.0">
         <xs:include schemaLocation="../types/JaNejType.xsd"/>
         
         <xs:element name="StopAktivMarkering" type="fase2.1:JaNejType">
            <xs:annotation>
               <xs:documentation>Transient element der angiver om et stop er aktivt. Ved opret/ændr er default 'Ja'. Fremsendes 'Nej' vil stop blive inaktiveret.  Værdisæt:  Ja = Stop aktivt Nej = Stop inaktivt</xs:documentation>
            </xs:annotation>
         </xs:element>
      </xs:schema>
