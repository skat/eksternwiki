<?xml version="1.0" encoding="UTF-8"?>
      
      <xs:schema xmlns:fase2.1="http://skat.dk/begrebsmodel/2009/01/15/"
                 xmlns:xs="http://www.w3.org/2001/XMLSchema"
                 targetNamespace="http://skat.dk/begrebsmodel/2009/01/15/"
                 elementFormDefault="qualified"
                 version="1.0">
            
            <xs:simpleType name="BeløbPositivNegativ11Decimaler2Type">
               <xs:annotation>
                  <xs:documentation>Angiver positive og negative beløb på 11 karakterer samt 2 decimaler. Værdierne ligger indenfor følgende område: -99.999.999.999,00 - 99.999.999.999,00</xs:documentation>
               </xs:annotation>
               <xs:restriction base="xs:decimal">
                  <xs:maxInclusive value="99999999999"/>
                  <xs:minInclusive value="-99999999999"/>
                  <xs:fractionDigits value="2"/>
               </xs:restriction>
            </xs:simpleType>
         </xs:schema>
