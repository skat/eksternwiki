<?xml version="1.0" encoding="UTF-8"?>
      <xs:schema xmlns:fase2.1="http://skat.dk/begrebsmodel/2009/01/15/"
                 xmlns:xs="http://www.w3.org/2001/XMLSchema"
                 targetNamespace="http://skat.dk/begrebsmodel/2009/01/15/"
                 elementFormDefault="qualified"
                 version="1.0">
         <xs:include schemaLocation="../types/TalHelType.xsd"/>
         
         <xs:element name="DokumentReferenceId" type="fase2.1:TalHelType">
            <xs:annotation>
               <xs:documentation>Transient element, der sættes af kalderen i forbindelse med oprettelse af nye dokumenter. Hver dokument får en nummer (reference id), som servicen så kan referere til i svaret på oprettelsen. På den måde kan kalderen mappe servicens dokumentID med ReferenceId.</xs:documentation>
            </xs:annotation>
         </xs:element>
      </xs:schema>
