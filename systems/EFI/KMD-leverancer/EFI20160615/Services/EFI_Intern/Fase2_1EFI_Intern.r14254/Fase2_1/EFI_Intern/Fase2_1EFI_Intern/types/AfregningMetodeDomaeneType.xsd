<?xml version="1.0" encoding="UTF-8"?>
      
      <xs:schema xmlns:fase2.1="http://skat.dk/begrebsmodel/2009/01/15/"
                 xmlns:xs="http://www.w3.org/2001/XMLSchema"
                 targetNamespace="http://skat.dk/begrebsmodel/2009/01/15/"
                 elementFormDefault="qualified"
                 version="1.0">
            
            <xs:simpleType name="AfregningMetodeDomæneType">
               <xs:annotation>
                  <xs:documentation>Kode for afregningsmetode  Liste med faste værdier (enum)  Værdisæt: NK: Nemkonto CH: Check BO: Bankoverførsel SR: Statsregnskab</xs:documentation>
               </xs:annotation>
               <xs:restriction base="xs:string">
                  <xs:maxLength value="2"/>
                  <xs:enumeration value="NK"/>
                  <xs:enumeration value="CH"/>
                  <xs:enumeration value="BO"/>
                  <xs:enumeration value="SR"/>
               </xs:restriction>
            </xs:simpleType>
         </xs:schema>
