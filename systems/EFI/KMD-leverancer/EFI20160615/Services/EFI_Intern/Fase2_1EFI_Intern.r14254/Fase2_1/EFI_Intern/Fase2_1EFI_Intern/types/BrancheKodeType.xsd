<?xml version="1.0" encoding="UTF-8"?>
      
      <xs:schema xmlns:fase2.1="http://skat.dk/begrebsmodel/2009/01/15/"
                 xmlns:xs="http://www.w3.org/2001/XMLSchema"
                 targetNamespace="http://skat.dk/begrebsmodel/2009/01/15/"
                 elementFormDefault="qualified"
                 version="1.0">
            
            <xs:simpleType name="BrancheKodeType">
               <xs:annotation>
                  <xs:documentation>6-cifret kode, som kategoriserer brancher og erhvervsgrupper efter internationale retningslinjer.</xs:documentation>
               </xs:annotation>
               <xs:restriction base="xs:integer">
                  <xs:totalDigits value="6"/>
               </xs:restriction>
            </xs:simpleType>
         </xs:schema>
