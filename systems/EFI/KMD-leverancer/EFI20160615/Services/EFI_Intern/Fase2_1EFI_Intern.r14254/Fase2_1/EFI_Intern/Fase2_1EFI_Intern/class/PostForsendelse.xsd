<?xml version="1.0" encoding="UTF-8"?>
      <xs:schema xmlns:fase2.1="http://skat.dk/begrebsmodel/2009/01/15/"
                 xmlns:xs="http://www.w3.org/2001/XMLSchema"
                 targetNamespace="http://skat.dk/begrebsmodel/2009/01/15/"
                 elementFormDefault="qualified"
                 attributeFormDefault="unqualified"
                 version="1.0">
         <xs:annotation>
               <xs:documentation>Oplysninger om fysisk forsendelse af post</xs:documentation>
            </xs:annotation>
         <xs:include schemaLocation="../types/TalHelType.xsd"/>
         <xs:include schemaLocation="../types/Tekst4Type.xsd"/>
         <xs:element name="PostForsendelsePortopriszoneKode" type="fase2.1:TalHelType">
               
               <xs:annotation>
                  <xs:documentation>Zone for brevporto. Anvendes til sortering og beregning af porto. Eksempel: 1=Danmark</xs:documentation>
               </xs:annotation>
            </xs:element>
         <xs:element name="PostForsendelsePostNummer" type="fase2.1:Tekst4Type">
               
               <xs:annotation>
                  <xs:documentation>Angiver postnummeret på forsendelsesadressen. Typisk vil det være danske postnumre, men undtagelsevis kan det også indeholde kombinationen af land og porto-zone, fx US-5.</xs:documentation>
               </xs:annotation>
            </xs:element>
      </xs:schema>
