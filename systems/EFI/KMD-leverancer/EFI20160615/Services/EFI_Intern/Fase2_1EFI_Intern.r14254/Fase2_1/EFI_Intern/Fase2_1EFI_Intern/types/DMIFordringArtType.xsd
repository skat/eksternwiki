<?xml version="1.0" encoding="UTF-8"?>
      
      <xs:schema xmlns:fase2.1="http://skat.dk/begrebsmodel/2009/01/15/"
                 xmlns:xs="http://www.w3.org/2001/XMLSchema"
                 targetNamespace="http://skat.dk/begrebsmodel/2009/01/15/"
                 elementFormDefault="qualified"
                 version="1.0">
            
            <xs:simpleType name="DMIFordringArtType">
               <xs:annotation>
                  <xs:documentation>Anvendes til at definere om en fordring er en Inddrivelsesfordring, Opkrævningsfordring, Modregningsfordring eller en Transport  Listen består af faste værdier (Enum)</xs:documentation>
               </xs:annotation>
               <xs:restriction base="xs:string">
                  <xs:maxLength value="4"/>
                  <xs:enumeration value="OPKR"/>
                  <xs:enumeration value="INDR"/>
                  <xs:enumeration value="MODR"/>
                  <xs:enumeration value="TRAN"/>
               </xs:restriction>
            </xs:simpleType>
         </xs:schema>
