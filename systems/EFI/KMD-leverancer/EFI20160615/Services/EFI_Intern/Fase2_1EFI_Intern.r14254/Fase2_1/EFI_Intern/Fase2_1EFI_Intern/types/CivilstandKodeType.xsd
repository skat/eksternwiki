<?xml version="1.0" encoding="UTF-8"?>
      
      <xs:schema xmlns:fase2.1="http://skat.dk/begrebsmodel/2009/01/15/"
                 xmlns:xs="http://www.w3.org/2001/XMLSchema"
                 targetNamespace="http://skat.dk/begrebsmodel/2009/01/15/"
                 elementFormDefault="qualified"
                 version="1.0">
            
            <xs:simpleType name="CivilstandKodeType">
               <xs:annotation>
                  <xs:documentation>Angiver om en person er død eller evt. genoplivet.  En død person har civilstandskode = D. En genoplivet person har tidligere haft civilstandkode = D</xs:documentation>
               </xs:annotation>
               <xs:restriction base="xs:string">
                  <xs:maxLength value="10"/>
                  <xs:pattern value="[UGPSFOEL]"/>
               </xs:restriction>
            </xs:simpleType>
         </xs:schema>
