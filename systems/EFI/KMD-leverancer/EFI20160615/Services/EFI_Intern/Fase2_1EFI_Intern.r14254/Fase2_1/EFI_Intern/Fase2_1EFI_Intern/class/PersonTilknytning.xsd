<?xml version="1.0" encoding="UTF-8"?>
      <xs:schema xmlns:fase2.1="http://skat.dk/begrebsmodel/2009/01/15/"
                 xmlns:xs="http://www.w3.org/2001/XMLSchema"
                 targetNamespace="http://skat.dk/begrebsmodel/2009/01/15/"
                 elementFormDefault="qualified"
                 attributeFormDefault="unqualified"
                 version="1.0">
         <xs:annotation>
               <xs:documentation>Personens familiemæssige tilknytning til andre personer - fx ægtefælle, børn m.m.</xs:documentation>
            </xs:annotation>
         <xs:include schemaLocation="../types/KodeType.xsd"/>
         <xs:element name="PersonTilknytningKode" type="fase2.1:KodeType">
               
               <xs:annotation>
                  <xs:documentation>Kode for de forskellige roller en persons familiemæssige relationer kan have: ægtefælle/reg. partner, barn, mor og far. </xs:documentation>
               </xs:annotation>
            </xs:element>
      </xs:schema>
