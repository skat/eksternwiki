<?xml version="1.0" encoding="UTF-8"?>
      
      <xs:schema xmlns:fase1="http://skat.dk/begrebsmodel/2006/09/07/"
                 xmlns:xs="http://www.w3.org/2001/XMLSchema"
                 targetNamespace="http://skat.dk/begrebsmodel/2006/09/07/"
                 elementFormDefault="qualified"
                 version="1.0">
            
            <xs:simpleType name="TastSelvKodeType">
               <xs:annotation>
                  <xs:documentation>Angiver en aktiveringskode, som dannes af sikkerhedsløsningen. Aktiveringskoden bruges herefter til at indtaste sin personlige TastSelv-kode i portalen. Aktiveringskoden har tilknyttet en udløbsdato. </xs:documentation>
               </xs:annotation>
               <xs:restriction base="xs:string">
                  <xs:maxLength value="16"/>
               </xs:restriction>
            </xs:simpleType>
         </xs:schema>
