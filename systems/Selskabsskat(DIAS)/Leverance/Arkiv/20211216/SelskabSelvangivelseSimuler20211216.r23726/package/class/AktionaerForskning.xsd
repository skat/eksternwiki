               </xs:annotation>
            </xs:element>
         <xs:element name="AktionærForskningSambeskattetNedsættelseUnderskudSkattekredit"
                     type="skat2012:JaNejType">
               
               <xs:annotation>
                  <xs:documentation>Tilkendegivelse af at underskud til fremførsel nedsættes tilsvarende det underskud der ligger til grund for ansøgning om skattekredit. Felt 612MJ</xs:documentation>
               </xs:annotation>
            </xs:element>
         <xs:element name="AktionærForskningVirksomhedPeriode" type="skat2012:JaNejType">
               
               <xs:annotation>
                  <xs:documentation>Tilkendegivelse af om der ansøges for en periode der er kortere end 12 måneder. Felt 607R</xs:documentation>
               </xs:annotation>
            </xs:element>
         <xs:element name="AktionærForskningVirksomhedPeriodeHent" type="skat2012:Tekst300Type">
               
               <xs:annotation>
                  <xs:documentation>Angiver den faktiske indkomstperiode som er registreret på selskabet i systemet. Felt 607RA</xs:documentation>
               </xs:annotation>
            </xs:element>
         <xs:element name="AktionærForskningVirksomhedSkattepligtigIndkomstFørUnderskud"
                     type="skat2012:BeløbPositivNegativ13Decimaler2Type">
               
               <xs:annotation>
                  <xs:documentation>Negativ skattepligtig indkomst før reduktion af underskud vedr. forsøgs- og forskningsaktiviteter. Felt 613R</xs:documentation>
               </xs:annotation>
            </xs:element>
         <xs:element name="AktionærForskningVirksomhedUnderskud"
                     type="skat2012:BeløbPositivNegativ13Decimaler2Type">
               
               <xs:annotation>
                  <xs:documentation>Angiv underskudsbeløb jf. udbetaling af skatteværdien af underskud, der stammer fra forsknings- og udviklingsudgifter, jf. ligningsloven. Felt 009e</xs:documentation>
               </xs:annotation>
            </xs:element>
         <xs:element name="AktionærForskningVirksomhedSkatteordning" type="skat2012:JaNejType">
               
               <xs:annotation>
                  <xs:documentation>Tilkendegivelse af om der i ansøgningen om skattekredit indgår virksomheder under virksomhedsskatteordningen. Felt 625</xs:documentation>
               </xs:annotation>
            </xs:element>
         <xs:element name="AktionærForskningVirksomhedSkatteværdiUnderskudAnsøgt"
                     type="skat2012:BeløbPositivNegativ13Decimaler2Type">
               
