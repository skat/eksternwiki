<?xml version="1.0" encoding="UTF-8"?>
      
      <xs:schema xmlns:skat2012="http://skat.dk/begrebsmodel/2012/01/01/"
                 xmlns:xs="http://www.w3.org/2001/XMLSchema"
                 targetNamespace="http://skat.dk/begrebsmodel/2012/01/01/"
                 elementFormDefault="qualified"
                 version="1.0">
            
            <xs:simpleType name="BeløbPositivNegativ15UdenDecimalerType">
               <xs:annotation>
                  <xs:documentation>Beløb som kan være positiv eller negativ og ingen decimaler har.  Beløb ligger indenfor grænsen: -999.999.999.999.999 - 999.999.999.999.999</xs:documentation>
               </xs:annotation>
               <xs:restriction base="xs:integer">
                  <xs:maxInclusive value="999999999999999"/>
                  <xs:minInclusive value="-999999999999999"/>
                  <xs:totalDigits value="15"/>
                  <xs:fractionDigits value="0"/>
               </xs:restriction>
            </xs:simpleType>
         </xs:schema>
