<?xml version="1.0" encoding="UTF-8"?>
      <xs:schema xmlns:skat2012="http://skat.dk/begrebsmodel/2012/01/01/"
                 xmlns:xs="http://www.w3.org/2001/XMLSchema"
                 targetNamespace="http://skat.dk/begrebsmodel/2012/01/01/"
                 elementFormDefault="qualified"
                 attributeFormDefault="unqualified"
                 version="1.0">
         <xs:annotation>
               <xs:documentation>Salg af varer og andre omsætningsaktiver.  (Gaver sidestilles med køb og salg).</xs:documentation>
            </xs:annotation>
         <xs:include schemaLocation="../types/MarkeringType.xsd"/>
         <xs:include schemaLocation="../types/KontrolleredeTransaktionerStatHjemmehoerendeKodeType.xsd"/>
         <xs:include schemaLocation="../types/BeloebType.xsd"/>
         <xs:element name="SalgAfVarerOgAndreOmsætningsaktiverOver25ProcentMarkering"
                     type="skat2012:MarkeringType">
               
               <xs:annotation>
                  <xs:documentation>Angiver om de kontrollerede transaktioner af denne art overstiger 25 % af virksomhedens totale transaktioner af denne art. Felt 510c</xs:documentation>
               </xs:annotation>
            </xs:element>
         <xs:element name="SalgAfVarerOgAndreOmsætningsaktiverStatHjemmehørendeKode"
                     type="skat2012:KontrolleredeTransaktionerStatHjemmehørendeKodeType">
               
               <xs:annotation>
                  <xs:documentation>Kode som angiver hvilke(n) stat(er) de enheder, der har været kontrollerede transaktioner med, er hjemmehørende - for den pågældende art transaktion. Felt 510d</xs:documentation>
               </xs:annotation>
            </xs:element>
         <xs:element name="SalgAfVarerOgAndreOmsætningsaktiverBrutto" type="skat2012:BeløbType">
               
               <xs:annotation>
                  <xs:documentation>Summen (beløb) af kontrollerede transaktioner for salg af varer og andre omsætningsaktiver. Felt 510b</xs:documentation>
               </xs:annotation>
            </xs:element>
         <xs:element name="SalgAfVarerOgAndreOmsætningsaktiverMarkering"
                     type="skat2012:MarkeringType">
               
               <xs:annotation>
                  <xs:documentation>Feltet tage stilling til om der været kontrollerede transaktioner af salg af varer og andre omsætningsaktiver. Felt 510a</xs:documentation>
               </xs:annotation>
            </xs:element>
      </xs:schema>
