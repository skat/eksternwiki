<?xml version="1.0" encoding="UTF-8"?>
      <xs:schema xmlns:skat2012="http://skat.dk/begrebsmodel/2012/01/01/"
                 xmlns:xs="http://www.w3.org/2001/XMLSchema"
                 targetNamespace="http://skat.dk/begrebsmodel/2012/01/01/"
                 elementFormDefault="qualified"
                 attributeFormDefault="unqualified"
                 version="1.0">
         <xs:annotation>
               <xs:documentation>Angiver status for en given virksomhed på et givet tidspunkt. Status kan f. eks være konkurs, likvidation, betalingsstandsning osv.</xs:documentation>
            </xs:annotation>
         <xs:include schemaLocation="../types/DatoType.xsd"/>
         <xs:element name="VirksomhedStatusForholdGyldigFra" type="skat2012:DatoType">
               
               <xs:annotation>
                  <xs:documentation>Angiver den første dag en virksomhedsstatustype er gyldig. </xs:documentation>
               </xs:annotation>
            </xs:element>
         <xs:element name="VirksomhedStatusForholdGyldigTil" type="skat2012:DatoType">
               
               <xs:annotation>
                  <xs:documentation>Angiver den sidste dag en virksomhedsstatustype er gyldig. </xs:documentation>
               </xs:annotation>
            </xs:element>
      </xs:schema>
