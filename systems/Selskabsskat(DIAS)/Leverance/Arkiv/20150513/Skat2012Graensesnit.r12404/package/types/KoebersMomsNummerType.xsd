<?xml version="1.0" encoding="UTF-8"?>
      
      <xs:schema xmlns:skat2012="http://skat.dk/begrebsmodel/2012/01/01/"
                 xmlns:xs="http://www.w3.org/2001/XMLSchema"
                 targetNamespace="http://skat.dk/begrebsmodel/2012/01/01/"
                 elementFormDefault="qualified"
                 version="1.0">
            
            <xs:simpleType name="KøbersMomsNummerType">
               <xs:annotation>
                  <xs:documentation>Købers momsnummer.  Det nummer som varemodtager er registreret med hos de nationale myndigheder i varemodtagers hjemland.   Hver varemodtager må kun optræde én gang hvert kvartal.</xs:documentation>
               </xs:annotation>
               <xs:restriction base="xs:string">
                  <xs:maxLength value="15"/>
               </xs:restriction>
            </xs:simpleType>
         </xs:schema>
