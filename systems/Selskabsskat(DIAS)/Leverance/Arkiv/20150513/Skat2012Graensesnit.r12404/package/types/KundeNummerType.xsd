<?xml version="1.0" encoding="UTF-8"?>
      
      <xs:schema xmlns:skat2012="http://skat.dk/begrebsmodel/2012/01/01/"
                 xmlns:xs="http://www.w3.org/2001/XMLSchema"
                 targetNamespace="http://skat.dk/begrebsmodel/2012/01/01/"
                 elementFormDefault="qualified"
                 version="1.0">
            
            <xs:simpleType name="KundeNummerType">
               <xs:annotation>
                  <xs:documentation>Et nummer, som for SKAT identificerer en person, virksomhed eller alternativ kontakt (person, virksomhed, myndighed, ukendt)</xs:documentation>
               </xs:annotation>
               <xs:restriction base="xs:string">
                  <xs:maxLength value="11"/>
                  <xs:pattern value="[0-9]{8,11}"/>
               </xs:restriction>
            </xs:simpleType>
         </xs:schema>
