<?xml version="1.0" encoding="UTF-8"?>
      
      <xs:schema xmlns:skat2012="http://skat.dk/begrebsmodel/2012/01/01/"
                 xmlns:xs="http://www.w3.org/2001/XMLSchema"
                 targetNamespace="http://skat.dk/begrebsmodel/2012/01/01/"
                 elementFormDefault="qualified"
                 version="1.0">
            
            <xs:simpleType name="Maks100Procent6DecimalerType">
               <xs:annotation>
                  <xs:documentation>0-100 med 6 decimaler (Anvendes til angivelse af procent</xs:documentation>
               </xs:annotation>
               <xs:restriction base="xs:decimal">
                  <xs:maxInclusive value="100"/>
                  <xs:minInclusive value="0"/>
                  <xs:fractionDigits value="6"/>
               </xs:restriction>
            </xs:simpleType>
         </xs:schema>
