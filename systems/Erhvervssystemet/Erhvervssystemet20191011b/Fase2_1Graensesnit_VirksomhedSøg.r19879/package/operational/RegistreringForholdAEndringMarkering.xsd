<?xml version="1.0" encoding="UTF-8"?>
      <xs:schema xmlns:fase2.1="http://skat.dk/begrebsmodel/2009/01/15/"
                 xmlns:xs="http://www.w3.org/2001/XMLSchema"
                 targetNamespace="http://skat.dk/begrebsmodel/2009/01/15/"
                 elementFormDefault="qualified"
                 version="1.0">
         <xs:include schemaLocation="../types/MarkeringType.xsd"/>
         
         <xs:element name="RegistreringForholdÆndringMarkering" type="fase2.1:MarkeringType">
            <xs:annotation>
               <xs:documentation>Transient element, hvormed man markerer, om man ønsker at fremsøge virksomheder, der har ændret registreringsforhold. Det vil sige, hvilke plgitforhold der er gældende for virksomheden (moms, punktafgifter m.m.)</xs:documentation>
            </xs:annotation>
         </xs:element>
      </xs:schema>
