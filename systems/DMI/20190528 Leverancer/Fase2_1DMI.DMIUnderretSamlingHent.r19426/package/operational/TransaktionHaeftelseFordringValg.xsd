<?xml version="1.0" encoding="UTF-8"?>
      <xs:schema xmlns:fase2.1="http://skat.dk/begrebsmodel/2009/01/15/"
                 xmlns:xs="http://www.w3.org/2001/XMLSchema"
                 targetNamespace="http://skat.dk/begrebsmodel/2009/01/15/"
                 elementFormDefault="qualified"
                 version="1.0">
         <xs:include schemaLocation="../types/TransaktionHaeftelseFordringValgType.xsd"/>
         
         <xs:element name="TransaktionHæftelseFordringValg"
                     type="fase2.1:TransaktionHæftelseFordringValgType">
            <xs:annotation>
               <xs:documentation>Angiver om en transaktion er en hæftelse eller en fordring. H: Hæftelse F: Fordring  Hvis transaktion vedrører en hæftelse vil KundeStruktur og HæftelseRestBeløb være udfyldt.  Hvis transaktion vedrører en fordring vil FordringRestBeløb være udfyldt.</xs:documentation>
            </xs:annotation>
         </xs:element>
      </xs:schema>
