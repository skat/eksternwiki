<restriction base="duration">
<pattern value="-?P([0-9]+Y)?(([1-9]|1[0-1])M)?(([1-9]|[1-2][0-9]|3[0-1])D)?(T(([1-9]|1[0-9]|2[0-3])H)?(([1-5]?[0-9])M)?((([1-5]?[0-9](\.[0-9]+)?))S)?)?"/>
</restriction>
</simpleType>
</schema>
