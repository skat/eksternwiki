<?xml version="1.0" encoding="UTF-8"?><schema xmlns="http://www.w3.org/2001/XMLSchema" xmlns:xkom="http://rep.oio.dk/xkom.dk/xml/schemas/2007/09/01/" xmlns:xkom-2006="http://rep.oio.dk/xkom.dk/xml/schemas/2006/09/01/" elementFormDefault="qualified" targetNamespace="http://rep.oio.dk/xkom.dk/xml/schemas/2007/09/01/">
	<include schemaLocation="DurationMeasure.xsd"/>
	<import namespace="http://rep.oio.dk/xkom.dk/xml/schemas/2006/09/01/" schemaLocation="../../../2006/09/01/StartDateTime.xsd"/>
	<import namespace="http://rep.oio.dk/xkom.dk/xml/schemas/2006/09/01/" schemaLocation="../../../2006/09/01/EndDateTime.xsd"/>
	<element name="TimeInterval" type="xkom:TimeIntervalType"/>
	<complexType name="TimeIntervalType">
		<choice>
			<sequence>
				<element ref="xkom-2006:StartDateTime"/>
				<choice minOccurs="0">
					<element ref="xkom-2006:EndDateTime"/>
					<element ref="xkom:DurationMeasure"/>
				</choice>
			</sequence>
			<sequence>
				<element ref="xkom:DurationMeasure"/>
				<element minOccurs="0" ref="xkom-2006:EndDateTime"/>
			</sequence>
			<element ref="xkom-2006:EndDateTime"/>
		</choice>
	</complexType>
</schema>
