<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:dmr="http://skat.dk/dmr/2007/05/31/" xmlns:xs="http://www.w3.org/2001/XMLSchema" targetNamespace="http://skat.dk/dmr/2007/05/31/" elementFormDefault="qualified" version="1.0">
	<xs:simpleType name="SynResultatType">
		<xs:annotation>
			<xs:documentation>Et syn kan medføre to resultater. Godkendt og Ikke Godkendt. Færdselsstyrelsen håndteres øvrige informationer - f.eks. beskrivelse af nedslidte bremser.  </xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:maxLength value="60"/>
			<xs:enumeration value="Godkendt"/>
			<xs:enumeration value="IkkeGodkendt"/>
			<xs:enumeration value="KanGodkendesVedOmsynAfOmsynsvirksomhed"/>
			<xs:enumeration value="KanGodkendesVedOmsynAfSynsvirksomhed"/>
			<xs:enumeration value="IkkeGodkendtKanGenfremstilles"/>
			<xs:enumeration value="IkkeFuldstaendigtSyn"/>
			<xs:whiteSpace value="collapse"/>
		</xs:restriction>
	</xs:simpleType>
</xs:schema>
