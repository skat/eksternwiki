<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:dmr="http://skat.dk/dmr/2007/05/31/" xmlns:xs="http://www.w3.org/2001/XMLSchema" targetNamespace="http://skat.dk/dmr/2007/05/31/" elementFormDefault="qualified" version="1.0">
	<xs:simpleType name="PeriodeHyppighedType">
		<xs:annotation>
			<xs:documentation>Beskriver en periode, feks daglig eller ugentlig  </xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:maxLength value="40"/>
			<xs:enumeration value="Daglig"/>
			<xs:enumeration value="Ugentlig"/>
			<xs:enumeration value="Månedlig"/>
			<xs:enumeration value="Kvartal"/>
			<xs:enumeration value="Halvårlig"/>
			<xs:enumeration value="Årlig"/>
		</xs:restriction>
	</xs:simpleType>
</xs:schema>
