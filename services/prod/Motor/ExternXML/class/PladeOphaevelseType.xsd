<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:dmr="http://skat.dk/dmr/2007/05/31/" xmlns:xs="http://www.w3.org/2001/XMLSchema" targetNamespace="http://skat.dk/dmr/2007/05/31/" elementFormDefault="qualified" attributeFormDefault="unqualified" version="1.0">
	<xs:annotation>
		<xs:documentation>En pladeophævelsestype er en gruppering af de relevante pladeophævelser.    1.        Køretøj afmeldt  2.        Køretøj ejer/brugerskiftet  3.        Ny gældende forsikring tilknyttet  4.        Køretøjet har godkendt synsresultat 6.        Plade afmeldt.  7.        Licens betalt.  8.        Licens overdraget 9.        Tilbagekaldt af SKAT</xs:documentation>
	</xs:annotation>
	<xs:include schemaLocation="../types/TalHelType.xsd"/>
	<xs:include schemaLocation="../types/TekstKortType.xsd"/>
	<xs:element name="PladeOphaevelseTypeNummer" type="dmr:TalHelType">
		<xs:annotation>
			<xs:documentation>Nummer på pladeophævelse type</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="PladeOphaevelseTypeNavn" type="dmr:TekstKortType">
		<xs:annotation>
			<xs:documentation>Navn på plade ophævelses type</xs:documentation>
		</xs:annotation>
	</xs:element>
</xs:schema>
