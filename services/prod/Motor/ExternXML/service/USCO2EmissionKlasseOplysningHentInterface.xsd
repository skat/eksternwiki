		<xs:choice>
			<xs:element ref="dmr:USCO2EmissionKlasseOplysningHent_I"/>
			<xs:element ref="dmr:USCO2EmissionKlasseOplysningHent_O"/>
			<xs:element ref="dmr:KompenserTrans"/>
			<xs:element ref="dmr:KompenserTransSvar"/>
		</xs:choice>
	</xs:complexType>
</xs:schema>
