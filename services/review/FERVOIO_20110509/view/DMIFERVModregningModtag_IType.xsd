								</xs:complexType>
							</xs:element>
						</xs:sequence>
						<xs:sequence>
							<xs:element name="NaesteUdaekketInternFordring">
								<xs:complexType>
									<xs:sequence>
										<xs:element ref="dmi:FoedevareErhvervFordringStiftelseDato"/>
										<xs:element ref="dmiws:FoedevareErhvervFordringRestbeloebStruktur"/>
									</xs:sequence>
								</xs:complexType>
							</xs:element>
						</xs:sequence>
					</xs:choice>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
