	</xs:annotation>
	<xs:import namespace="http://rep.oio.dk/skat.dk/basis/kontekst/xml/schemas/2006/09/01/" schemaLocation="http://digitaliser.dk/resource/247441/artefact/SKAT_HovedOplysninger.xsd"/><xs:import namespace="urn:oio:skat:dmi:1.0.0" schemaLocation="../class/FoedevareErhvervTransportBeloebIndikator.xsd"/><xs:import namespace="urn:oio:skat:dmi:1.0.0" schemaLocation="../class/FoedevareErhvervTransportModtagelseDato.xsd"/><xs:import namespace="urn:oio:skat:dmi:1.0.0" schemaLocation="../class/NemKontoUdbetalingBankdagDato.xsd"/><xs:import namespace="urn:oio:skat:dmi:1.0.0" schemaLocation="../class/FoedevareErhvervFordringStiftelseDato.xsd"/><xs:include schemaLocation="../view/KundeStrukturType.xsd"/>
	<xs:include schemaLocation="../view/MyndighedUdbetalingBeloebStrukturType.xsd"/>
	<xs:include schemaLocation="../view/FoedevareErhvervFordringRestbeloebStrukturType.xsd"/>
	<xs:element name="DMIFERVModregningModtag_I" type="dmiws:DMIFERVModregningModtag_IType">
	</xs:element>
	<xs:complexType name="DMIFERVModregningModtag_IType">
		<xs:sequence>
			<xs:element ref="kontekst:HovedOplysninger"/>
			<xs:element ref="dmiws:KundeStruktur"/>
