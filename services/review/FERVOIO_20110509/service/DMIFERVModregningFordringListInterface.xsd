		<xs:choice>
			<xs:element ref="dmiws:DMIFERVModregningFordringList_I"/>
			<xs:element ref="dmiws:DMIFERVModregningFordringList_O"/>
		</xs:choice>
	</xs:complexType>
</xs:schema>
