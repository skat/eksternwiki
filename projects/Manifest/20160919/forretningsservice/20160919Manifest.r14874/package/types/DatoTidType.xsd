<?xml version="1.0" encoding="UTF-8"?>
      
      <xs:schema xmlns:fase2.1="http://skat.dk/begrebsmodel/2009/01/15/"
                 xmlns:xs="http://www.w3.org/2001/XMLSchema"
                 targetNamespace="http://skat.dk/begrebsmodel/2009/01/15/"
                 elementFormDefault="qualified"
                 version="1.0">
            
            <xs:simpleType name="DatoTidType">
               <xs:annotation>
                  <xs:documentation>En datotid datatype, som samlet betegner en dato og tid, med formatet dd-mm-yyyy hh:mm:ss</xs:documentation>
               </xs:annotation>
               <xs:restriction base="xs:dateTime">
                  <xs:whiteSpace value="collapse"/>
               </xs:restriction>
            </xs:simpleType>
         </xs:schema>
