<?xml version="1.0" encoding="UTF-8"?>
      <xs:schema xmlns:fase2.1="http://skat.dk/begrebsmodel/2009/01/15/"
                 xmlns:xs="http://www.w3.org/2001/XMLSchema"
                 targetNamespace="http://skat.dk/begrebsmodel/2009/01/15/"
                 elementFormDefault="qualified"
                 version="1.0">
         <xs:include schemaLocation="../types/DatoType.xsd"/>
         
         <xs:element name="HovedOpkrævningFordringSidsteRettidigBetalingDato"
                     type="fase2.1:DatoType">
            <xs:annotation>
               <xs:documentation>Hovedfordringens SidsteRettidigBetalingDato, som er den sidste frist for, hvornår en fordring skal være betalt.  Sidste rettidige betalingsdato - også kaldet SRB - er den rentebærende dato, dvs. den dato, hvorfra der evt. skal beregnes rente. SidsteRettidigBetalingDato er ikke altid lig med ForfaldDato</xs:documentation>
            </xs:annotation>
         </xs:element>
      </xs:schema>
