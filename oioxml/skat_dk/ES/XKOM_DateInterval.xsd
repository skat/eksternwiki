<?xml version="1.0" encoding="UTF-8"?>
<schema targetNamespace="http://rep.oio.dk/xkom.dk/xml/schemas/2007/09/01/" xmlns:xkom="http://rep.oio.dk/xkom.dk/xml/schemas/2007/09/01/" xmlns:xkom2="http://rep.oio.dk/xkom.dk/xml/schemas/2007/04/15/" xmlns:xkom-2006="http://rep.oio.dk/xkom.dk/xml/schemas/2006/09/01/" xmlns="http://www.w3.org/2001/XMLSchema" elementFormDefault="qualified">
	<include schemaLocation="XKOM_DurationMeasure.xsd"/>
	<import namespace="http://rep.oio.dk/xkom.dk/xml/schemas/2006/09/01/" schemaLocation="XKOM_StartDate.xsd"/>
	<import namespace="http://rep.oio.dk/xkom.dk/xml/schemas/2007/04/15/" schemaLocation="XKOM_EndDate.xsd"/>
	<element name="DateInterval" type="xkom:DateIntervalType"/>
	<complexType name="DateIntervalType">
		<choice>
			<sequence>
				<element ref="xkom-2006:StartDate"/>
				<choice minOccurs="0">
					<element ref="xkom2:EndDate"/>
					<element ref="xkom:DurationMeasure"/>
				</choice>
			</sequence>
			<sequence>
				<element ref="xkom:DurationMeasure" minOccurs="0"/>
				<element ref="xkom2:EndDate"/>
			</sequence>
		</choice>
	</complexType>
</schema>
